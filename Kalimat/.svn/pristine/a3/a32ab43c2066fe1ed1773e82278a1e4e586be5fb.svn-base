}
.ordered_list_en /* for english ordered list */ {
	direction: ltr;
}
.ordered_list_ar /* for arabic ordered list */ {
	direction: rtl;
}
.variable_list /* for variable list*/ {
	line-height: 1.75em;
	margin-top: 12px;
}
.variable_list div.Div_P {
