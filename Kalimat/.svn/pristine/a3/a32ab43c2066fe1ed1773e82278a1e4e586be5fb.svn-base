}
/****************************************************************************************************
							Images nodes
*****************************************************************************************************/
.mediaobject /* for media object node that come inside informalfigure*/ {
	width: 80%;
	height: 60%;
	margin-right: 10%;
	border: 0px solid;
}
.mediaobject p {
	text-align: center;
}
.figurecaption {
	text-align: center;
	margin-top: -0.5em;
	margin-bottom: 1em;
	line-height: 1.5em;
}
.figure /* for image*/ {
	margin-top: 1em;
	margin-right: 0em;
	margin-left: 0em;
	margin-bottom: 1em;
	border: 0px solid;
}
.inline_figure /* for image*/ {
	border: 0px solid;
	height: 1.2em;
	display: inline;
}
.full-screen {
	top: 0;
	bottom: 0;
	page-break-after: always;
	page-break-before: always;
}
/****************************************************************************************************
							Bridge head nodes
*****************************************************************************************************/
.bridge_head_en /* for english bridge head */ {
	direction: ltr;
	font-weight: bold;
	margin-top: 1.25em;
	text-align: left;
}
.bridge_head_ar /* for arabic bridge head */ {
	direction: rtl;
	font-weight: bold;
	margin-top: 1.25em;
	margin-bottom: 0.75em;
	text-align: right;
}
.bridge_head_center  /* for bridge head center (******) */ {
	width: 100%;
	display: inline-block;
	direction: rtl;
	margin-top: 0.5em;
}
/****************************************************************************************************
							Dialog nodes
*****************************************************************************************************/
.dialogue /* for linegroup that appear inside dialogue (e.g  dialogue/linegroup )*/ {
	clear: both;
	line-height: 1.75em;
	margin-top: 0.75em;
}
.speaker /* for speaker inside linegroup inside dialogue (e.g  dialogue/linegroup/speaker )*/ {
	font-weight: bold;
	display: inline;
}
.speach /* for line inside linegroup inside dialogue (e.g  dialogue/linegroup/line )*/ {
	display: inline;
	border: 0px solid;
	margin-bottom: 0.75em;
}
/****************************************************************************************************
							Footnote nodes
*****************************************************************************************************/
.bookmargin /* for footnote (the box that contains all footnotes at the end of each chapter) */ {
	font-size: 1em;
	margin-top: 12px;
	width: 100%;
	line-height: 1.75em;
}
.bookmargin a /* for footnote (the anchor that appear inside bookmargin) */ {
	font-size: 0.9em;
	text-decoration: none;
}
.footnote a /* for footnote anchor*/ {
	font-size: 0.9em;
	text-decoration: none;
}
.footnote {
	display: inline-block;
}
.footnote_line /* for footnote line (the line that appear at each chapter before writing footnotes)*/ {
	border-top: 1px solid #CCCCCC;
	height: 2px;
	width: 25%;
	margin-top: 2em;
}
/****************************************************************************************************
							Table nodes
*****************************************************************************************************/
.table  /* for table and informaltable*/ {
	width: auto;
	margin: 1em auto;/*margin-bottom:1em;
	margin-top:1em;*/
}
.table td /* for td that appear inside any table*/ {
	line-height: 1.6em;
	padding-left: 1em;
	padding-top: 0.25em;
	padding-bottom: 0.25em;
	text-align: right;
}
.table th /* for th that appear inside any table*/ {
	line-height: 1.5em;
	padding-left: 1em;
	padding-top: 0.25em;
	padding-bottom: 0.25em;
	text-align: right;/*width: 50%;*/
}
.table_caption {
	text-align: center;
	margin-bottom: -0.5em;
	margin-top: 1em;
	line-height: 1.5em;
}
/****************************************************************************************************
